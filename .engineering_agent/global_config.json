{
  "version": "1.0.0",
  "default_settings": {
    "test_timeout": 300,
    "test_coverage_threshold": 80.0,
    "code_quality_threshold": 8.0,
    "notifications_enabled": true,
    "auto_deploy_on_success": false
  },
  "integrations": {
    "github": {
      "enabled": true,
      "api_base": "https://api.github.com"
    },
    "jira": {
      "enabled": true,
      "api_version": "3"
    },
    "confluence": {
      "enabled": true,
      "api_version": "latest"
    }
  }
}